# Databricks Configuration
# Copy this file to .env and fill in your actual values

# Your Databricks workspace URL (e.g., adb-1234567890123456.7.azuredatabricks.net)
DATABRICKS_SERVER_HOSTNAME=your-databricks-workspace-url

# HTTP path for SQL endpoint (e.g., /sql/1.0/warehouses/your-warehouse-id)
DATABRICKS_HTTP_PATH=your-http-path

# Personal Access Token from Databricks
DATABRICKS_ACCESS_TOKEN=your-personal-access-token

# Catalog name (default: main)
DATABRICKS_CATALOG=main

# Schema name (default: default)
DATABRICKS_SCHEMA=default

# Table name for agent executions (default: agent_executions)
AGENT_EXECUTIONS_TABLE=agent_executions
